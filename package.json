{
  "name": "@csc530/vuebulma",
  "version": "0.0.0-7.1",
  "author": "Christofer Cousins",
  "description": "Bulma CSS framework components for Vue 3",
  "keywords": [
    "vue3",
    "Bulma",
    "vue3 component library",
    "Vue",
    "components",
    "component library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csc530/vuebulma.git"
  },
  "homepage": "https://csc530.github.io/vuebulma/",
  "bugs": {
    "url": "https://github.com/csc530/vuebulma/issues",
    "email": "csc530@vivaldi.net"
  },
  "sponsor": {
    "url": "https://paypal.me/csc530"
  },
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "dist/vuebulma.js",
  "module": "dist/vuebulma.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/vuebulma.esm.js",
      "require": "./dist/vuebulma.umd.cjs",
      "default": "./dist/vuebulma.js"
    },
    "./types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite --host --open -d --clearScreen",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@csc530/vuebulma": "0.0.0-7.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vuepress/client": "2.0.0-beta.60",
    "bulma": "^0.9.4",
    "change-case": "^4.1.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.4",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.0.13",
    "vuepress": "2.0.0-beta.60",
    "vuepress-plugin-clipboard": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  }
}
