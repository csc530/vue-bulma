{
  "name": "@csc530/vuebulma",
  "version": "0.0.0-6",
  "author": "Christofer Cousins",
  "description": "Bulma CSS framework components for Vue 3",
  "keywords": [
	"vue3",
	"Bulma",
	"vue3 component library",
	"Vue",
	"components",
	"component library"
  ],
  "repository": {
	"type": "git",
	"url": "https://github.com/csc530/vue-bulma.git"
  },
  "homepage": "https://github.com/csc530/vue-bulma#readme",
  "bugs": {
	"url": "https://github.com/csc530/vuebulma/issues",
	"email": "csc530@vivaldi.net"
  },
  "sponsor": {
	"url": "https://paypal.me/csc530"
  },
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "dist/vuebulma.js",
  "module": "dist/vuebulma.umd.cjs",
  "exports": {
	".": {
	  "import": "./dist/vuebulma.js",
	  "require": "./dist/vuebulma.umd.cjs"
	}
  },
  "files": [
	"dist/*"
  ],
  "scripts": {
	"dev": "vite --host --open -d --clearScreen",
	"build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
	"preview": "vite preview"
  },
  "dependencies": {
	"vue": "^3.2.45"
  },
  "devDependencies": {
	"@vitejs/plugin-vue": "^3.1.0",
	"bulma": "^0.9.4",
	"typescript": "^4.9.3",
	"vite": "^3.2.4",
	"vue-loader": "^16.8.3",
	"vue-tsc": "^1.0.10",
	"yarn": "^1.22.19"
  },
  "peerDependencies": {
	"vue": "^3.2.45"
  }
}
